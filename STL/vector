/// how to input queue in c++ from stack array in c.....

#include<bits/stdc++.h>
using namespace std;

int main()
{
    queue<int> st;
    int n,j,i;

    cin>>n;
    for ( i = 0; i<n; i++) {
        cin>>j;
        st.push(j);
    }

    while( !st.empty()) {

        cout<<st.front()<<" ";
        st.pop();
    }
    return 0;
}


/// how to input stack in c++ from stack array in c.....
#include<bits/stdc++.h>
using namespace std;

int main()
{
    stack<int> st;
    int n,j,i;

    cin>>n;
    for ( i = 0; i<n; i++) {
        cin>>j;
        st.push(j);
    }

    while( !st.empty()) {

        cout<<st.top()<<" ";
        st.pop();
    }
    return 0;
}
